                        </login-module>
                        <login-module code="RealmDirect" flag="required">
                            <!-- <module-option name="password-stacking" value="useFirstPass"/> -->
                            <module-option name="unauthenticatedIdentity" value="guest"/>
                        </login-module>
                    </authentication>
                </security-domain>
